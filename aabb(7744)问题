/*aabb
输出所有形如aabb的4位完全平方数（即前两位数字相等，后两位数字也相等）。
*/
#include<stdio.h>
#include<math.h>
int main()
{
	int a,b;
	for(a=1;a<=9;a++)
		for(b=0;b<=9;b++)
		{
			int n = a*1100 +b*11;
			//函数floor返回不超过x的最大整数 
			int m = floor(sqrt(n)+0.5);//sqrt(n)+0.5四舍五入后面的整数。 
			if(m*m == n)	printf("%d\n",n);
		}
	return 0;
}

/*枚举平方根x*/
#include<stdio.h>
int main()
{
	int x;
	for(x=1;;x++)
	{
		int n=x*x;
		if(n<1000) continue;
		if(n>99990) break;
		int hi=n/100;
		int lo=n%100;
		if(hi/10 == hi%10 && lo/10 == lo%10) printf("%d\n", n);
	}
	return 0;
}
