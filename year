/*年份（year）
输入年份，判断是否为闰年。如果是，则输出yes，否则输出no。
*/
#include<stdio.h>
int main()
{
	int years;
	scanf("%d",&years);
	if(years%4 == 0)
	{
		if(years%100 != 0)
		{
			printf("no\n");
		}
		else
		{
			if(years%400 == 0)
			{
				printf("yes\n");
			}
			else
			{
				printf("no\n");
			}
		}
	}
	else
	{
		printf("no\n");
	}
	return 0;
}
